<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">

    <servlet>
        <servlet-name>AddNewGoodServlet</servlet-name>
        <servlet-class>servlets.AddNewGoodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddNewGoodServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserMenuServlet</servlet-name>
        <servlet-class>servlets.UserMenuServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserMenuServlet</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginPasswordServlet</servlet-name>
        <servlet-class>servlets.LoginPasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginPasswordServlet</servlet-name>
        <url-pattern>/LoginPasswordServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShopTmp</servlet-name>
        <servlet-class>servlets.ShopTmp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ShopTmp</servlet-name>
        <url-pattern>/ShopTmp</url-pattern>
    </servlet-mapping>
</web-app>